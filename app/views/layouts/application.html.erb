<!DOCTYPE html>
<html>
<head>
  <title>FoodDonate</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <script src="http://maps.google.com/maps/api/js"></script>
  <%= csrf_meta_tags %>
  <meta name="viewport" content="width=device-width, initial-scale=1">
</head>
<body ng-app="app">
  <div>
    <%= render 'static_pages/nav' %>
    <div class="row clearfix">
      <%- unless notice.nil? %>
      <div class="alert alert-success" role="alert"><%= notice %></div>
      <% end -%>
      <%- unless alert.nil? %>
      <div class="alert alert-danger" role="alert"><%= alert %></div>
      <% end -%>
    </div>
    <%= yield %>
  </div>


<div class="row">
  <div class="fh-footer text-center">
    <div class="row">
      <div class="col-md-3 col-sm-3 col-xs-3 fh-footer-items">Home</div>
      <div class="col-md-3 col-sm-3 col-xs-3 fh-footer-items">About</div>
      <div class="col-md-3 col-sm-3 col-xs-3 fh-footer-items">Site Map</div>
      <div class="col-md-3 col-sm-3 col-xs-3 fh-footer-items">Contact
        <ul class="list-unstyled">
          <li class="fh-address">123 Congress Ave</li>
          <li class="fh-address">Austin, TX 78704</li>
          <li>
            <span class="fh-circle"></span>
            <span class="fh-circle"></span>
            <span class="fh-circle"></span>
          </li>
        </ul>
      </div>
    </div>
  </div>
</div>




</body>
</html>
